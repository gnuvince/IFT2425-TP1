digraph Matrix {
    node [shape = "record"];
    m [label="<elems> elems | <start> start | <rows> rows | <cols> cols"];
    rows [label="<0> 0 | <1> 1 | <2> 2"];
    elems [label="<0> 0|1|2|<3> 3|4|5|<6> 6|7|8"];

    m:elems -> rows:0;
    m:start -> elems:0;
    rows:0 -> elems:0;
    rows:1 -> elems:3;
    rows:2 -> elems:6;

}
